{
  "name": "chrome-extension-vue-vuetify",
  "version": "0.0.1",
  "description": "Template for creating chrome extensions using vue + vuetify",
  "author": "talal424",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore src",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore src",
    "prettier": "prettier \"src/**/*.{js,vue}\"",
    "prettier:write": "npm run prettier -- --write",
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules",
    "build:zip": "npm run build & node scripts/build-zip.js",
    "dev": "npm run build:dev -- --watch"
  },
  "homepage": "https://github.com/talal424/chrome-extension-vue-vuetify",
  "bugs": {
    "url": "https://github.com/talal424/chrome-extension-vue-vuetify/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/talal424/chrome-extension-vue-vuetify.git"
  },
  "keywords": [
    "vuetify",
    "vue",
    "component",
    "chrome",
    "extension"
  ],
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "dependencies": {
    "vue": "^2.6.10",
    "vuetify": "^2.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime-corejs3": "^7.4.0",
    "@mdi/js": "^5.9.55",
    "archiver": "^5.2.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.4.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "eslint-webpack-plugin": "^2.4.3",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^4.5.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "sass": "^1.32.5",
    "sass-loader": "^8.0.2",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.6.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.3.10"
  }
}
